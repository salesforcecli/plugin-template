{
  "name": "@salesforce/plugin-template",
  "description": "A template repository for sfdx plugins",
  "version": "2.1.0",
  "main": "lib/index.js",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^1.22.0",
    "@salesforce/command": "^5.2.35",
    "@salesforce/core": "^3.32.12",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.2.13",
    "@salesforce/cli-plugins-testkit": "^3.2.15",
    "@salesforce/dev-config": "^3.0.1",
    "@salesforce/dev-scripts": "^3.1.0",
    "@salesforce/plugin-command-reference": "^1.5.10",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.4.2",
    "@swc/core": "^1.3.25",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.48.0",
    "chai": "^4.3.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "oclif": "^3.4.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.0",
    "shx": "0.2.2",
    "sinon": "^11.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.9.4"
  },
  "config": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-template",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    }
  },
  "repository": "salesforcecli/plugin-template",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  }
}
